    "@types/node-telegram-bot-api": "^0.64.8",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "node-telegram-bot-api": "^0.66.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.4",
